{
	"name": "DimRegionLoad",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dimregion",
						"type": "DatasetReference"
					},
					"name": "CountryCodesTable"
				},
				{
					"dataset": {
						"referenceName": "DimRegion1",
						"type": "DatasetReference"
					},
					"name": "DimRegion"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DimRegion1",
						"type": "DatasetReference"
					},
					"name": "DimRegionLoad"
				}
			],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "aggregate1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_JOB_ID as string",
				"}",
				"source(output(",
				"          Code as string,",
				"          Country_name as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CountryCodesTable",
				"source(output(",
				"          MAXREGIONKEY as decimal(10,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT NVL(MAX(REGIONKEY),0) as MAXREGIONKEY from IMDB_SCHEMA.DIMREGION',",
				"     format: 'query') ~> DimRegion",
				"CountryCodesTable, DimRegion join(1==1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1 aggregate(groupBy(Code,",
				"          Country_name,",
				"          MAXREGIONKEY),",
				"     CountRow = count(Code)) ~> aggregate1",
				"aggregate1 keyGenerate(output(RowNum as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(REGIONKEY = MAXREGIONKEY + RowNum,",
				"          DI_JOB_ID = $DI_JOB_ID,",
				"          DI_CREATED_DT = currentDate()) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          REGIONKEY,",
				"          Code,",
				"          Country_name,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT",
				"     )) ~> DimRegionLoad"
			]
		}
	}
}