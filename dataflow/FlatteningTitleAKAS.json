{
	"name": "FlatteningTitleAKAS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "STAGING_TITLE_AKAS",
						"type": "DatasetReference"
					},
					"name": "FlatteningTitleAkas"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TITLEID as string,",
				"          ORDERING as string,",
				"          TITLE as string,",
				"          REGION as string,",
				"          LANGUAGE as string,",
				"          TYPES as string,",
				"          ATTRIBUTES as string,",
				"          ISORIGINALTITLE as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     partitionBy('roundRobin', 2)) ~> FlatteningTitleAkas",
				"FlatteningTitleAkas derive(REGION_NEW = split(REGION,',')) ~> derivedColumn1",
				"derivedColumn1 foldDown(unroll(REGION_NEW),",
				"     mapColumn(",
				"          REGION_NEW = REGION_NEW,",
				"          TITLEID",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}