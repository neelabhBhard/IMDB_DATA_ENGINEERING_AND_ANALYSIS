{
	"name": "FlattenTitleBasic",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "SnowflakeTable_imdb",
						"type": "DatasetReference"
					},
					"name": "FlatteningTitleBasic"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as string,",
				"          STARTYEAR as string,",
				"          ENDYEAR as string,",
				"          RUNTIMEMINUTES as string,",
				"          GENRES as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     format: 'table') ~> FlatteningTitleBasic",
				"FlatteningTitleBasic derive(GENRES_NEW = split(GENRES,','),",
				"     partitionBy('roundRobin', 2)) ~> derivedColumn1",
				"derivedColumn1 foldDown(unroll(GENRES_NEW),",
				"     mapColumn(",
				"          GENRES_NEW = GENRES_NEW,",
				"          TCONST",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1"
			]
		}
	}
}