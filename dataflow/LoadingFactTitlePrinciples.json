{
	"name": "LoadingFactTitlePrinciples",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "STAGE_TITLE_PRINCIPLE",
						"type": "DatasetReference"
					},
					"name": "TITLEPRINCIPLE"
				},
				{
					"dataset": {
						"referenceName": "ADDINGDIMTITLE",
						"type": "DatasetReference"
					},
					"name": "DIMTITLE"
				},
				{
					"dataset": {
						"referenceName": "DIMNAME",
						"type": "DatasetReference"
					},
					"name": "DIMNAME"
				},
				{
					"dataset": {
						"referenceName": "LOADINGFACTPRINCIPAL",
						"type": "DatasetReference"
					},
					"name": "FACTPRINCIPLES"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "LOADFACTPRINCIPAL3",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "JOINING1"
				},
				{
					"name": "join1"
				},
				{
					"name": "join2"
				},
				{
					"name": "select1"
				},
				{
					"name": "surrogateKey1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_LOAD_ID as string",
				"}",
				"source(output(",
				"          TCONST as string,",
				"          ORDERING as string,",
				"          NCONST as string,",
				"          CATEGORY as string,",
				"          JOB as string,",
				"          CHARACTERS as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> TITLEPRINCIPLE",
				"source(output(",
				"          TITLEKEY as decimal(10,0),",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as string,",
				"          STARTYEAR as string,",
				"          ENDYEAR as string,",
				"          RUNTIMEMINUTES as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> DIMTITLE",
				"source(output(",
				"          NAMEKEY as decimal(10,0),",
				"          NCONST as string,",
				"          PRIMARYNAME as string,",
				"          BIRTHYEAR as string,",
				"          DEATHYEAR as string,",
				"          KNOWNFORTITLES as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> DIMNAME",
				"source(output(",
				"          PRINCIPLESK as decimal(10,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'SELECT NVL(MAX(PRINCIPALKEY),0) AS PRINCIPLESK FROM FactTitlePrincipals',",
				"     format: 'query') ~> FACTPRINCIPLES",
				"TITLEPRINCIPLE, DIMTITLE join(TITLEPRINCIPLE@TCONST == DIMTITLE@TCONST,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> JOINING1",
				"JOINING1, DIMNAME join(TITLEPRINCIPLE@NCONST == DIMNAME@NCONST,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1, FACTPRINCIPLES join(1 == 1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"join2 select(mapColumn(",
				"          PRINCIPLESK,",
				"          TITLEKEY,",
				"          NAMEKEY,",
				"          ORDERING,",
				"          CATEGORY,",
				"          JOB,",
				"          CHARACTERS",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 keyGenerate(output(ROWNUM as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"surrogateKey1 derive(PRINCIPLESK = PRINCIPLESK + ROWNUM,",
				"          DI_LOAD_ID = $DI_LOAD_ID,",
				"          DI_CREATED_DT = currentDate()) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          PRINCIPALKEY = PRINCIPLESK,",
				"          TITLEKEY,",
				"          NAMEKEY,",
				"          ORDERING,",
				"          CATEGORY,",
				"          JOB,",
				"          CHARACTERS,",
				"          DI_LOAD_ID,",
				"          DI_CREATED_DT",
				"     )) ~> sink1"
			]
		}
	}
}