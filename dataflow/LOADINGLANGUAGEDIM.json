{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "LOADINGLANGUAGE",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "LOADINGLANGUAGEDIM",
						"type": "DatasetReference"
					},
					"name": "LOADINGDIMLANGUAGE"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "surrogateKey1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_JOB_ID as string",
				"}",
				"source(output(",
				"          {ISO Language Names} as string,",
				"          {Set 1} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"surrogateKey1 derive(LANGUAGE_NEW = regexReplace({ISO Language Names}, '[^a-zA-Z0-9\\\\s\\\\(\\\\)\\\\?,]', ' '),",
				"          DI_JOB_ID = $DI_JOB_ID,",
				"          DI_CREATED_DT = currentDate()) ~> derivedColumn1",
				"source1 keyGenerate(output(LANG_ID as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          LANGUAGEKEY = LANG_ID,",
				"          LANGUAGECODE = {Set 1},",
				"          LANGUAGEDESCRIPTION = LANGUAGE_NEW,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT",
				"     )) ~> LOADINGDIMLANGUAGE"
			]
		}
	}
}