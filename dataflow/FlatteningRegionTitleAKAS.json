{
	"name": "FlatteningRegionTitleAKAS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "IMPORTING_TITLE_CREW",
						"type": "DatasetReference"
					},
					"name": "FlatteningTitleAkas"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "LOADINGTITLE_CREW_V2",
						"type": "DatasetReference"
					},
					"name": "LoadingTitleCrewV2"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TCONST as string,",
				"          DIRECTORS as string,",
				"          WRITERS as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     partitionBy('roundRobin', 2)) ~> FlatteningTitleAkas",
				"FlatteningTitleAkas derive(WRITERS_NEW = split(WRITERS,','),",
				"          DIRECTORS_NEW = split(DIRECTORS,',')) ~> derivedColumn1",
				"derivedColumn1 foldDown(unrollMultiple(WRITERS_NEW,DIRECTORS_NEW),",
				"     mapColumn(",
				"          TCONST,",
				"          DIRECTORS_NEW,",
				"          WRITERS_NEW,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"flatten1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          TCONST,",
				"          DIRECTORS_NEW,",
				"          WRITERS_NEW,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT",
				"     )) ~> LoadingTitleCrewV2"
			]
		}
	}
}