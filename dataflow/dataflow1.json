{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DimTitle",
						"type": "DatasetReference"
					},
					"name": "StgTitleBasicV2"
				},
				{
					"dataset": {
						"referenceName": "DimTitlenew",
						"type": "DatasetReference"
					},
					"name": "DimTitleN"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          TCONST as string,",
				"          TITLETYPE as string,",
				"          PRIMARYTITLE as string,",
				"          ORIGINALTITLE as string,",
				"          ISADULT as string,",
				"          STARTYEAR as string,",
				"          ENDYEAR as string,",
				"          RUNTIMEMINUTES as string,",
				"          GENRES as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table') ~> StgTitleBasicV2",
				"source(output(",
				"          TITLEKEY as decimal(10,0)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     query: 'Select NVL(Max(Titlekey),0) as Titlekey from IMDB_SCHEMA.DIMTITLE',",
				"     format: 'query') ~> DimTitleN",
				"StgTitleBasicV2, DimTitleN join(1==1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1"
			]
		}
	}
}