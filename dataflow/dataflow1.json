{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "name_basics",
						"type": "DatasetReference"
					},
					"name": "namebasics"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "STAGINGNAMEBASIC",
						"type": "DatasetReference"
					},
					"name": "LOADINGNAMEBASIC"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				},
				{
					"name": "flatten1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          NCONST as string,",
				"          PRIMARYNAME as string,",
				"          BIRTHYEAR as string,",
				"          DEATHYEAR as string,",
				"          PRIMARYPROFESSION as string,",
				"          KNOWNFORTITLES as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     partitionBy('roundRobin', 2)) ~> namebasics",
				"namebasics derive(Profession_new = split(PRIMARYPROFESSION, ',')) ~> derivedColumn1",
				"derivedColumn1 foldDown(unroll(Profession_new),",
				"     mapColumn(",
				"          NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          Profession_new,",
				"          KNOWNFORTITLES,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT",
				"     ),",
				"     skipDuplicateMapInputs: false,",
				"     skipDuplicateMapOutputs: false) ~> flatten1",
				"flatten1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          NCONST,",
				"          PRIMARYNAME,",
				"          BIRTHYEAR,",
				"          DEATHYEAR,",
				"          PROFESSION = Profession_new,",
				"          KNOWNFORTITLES,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT",
				"     )) ~> LOADINGNAMEBASIC"
			]
		}
	}
}